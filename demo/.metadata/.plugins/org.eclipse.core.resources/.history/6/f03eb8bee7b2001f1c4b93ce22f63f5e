package number;

public class Palindrom {
	public boolean isBolean(int x)
	{
		if(n<0)
		{
			return false;
		}
		int n=x;
		int revNum=0;
		while(n>0)
		{
			int d=n%10;
			revNum=revNum%10+d;
			n=n/10;
		}
		if(revNum==x)
		{
			return true;
			
		}
	}
